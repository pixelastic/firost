{
  "name": "firost",
  "version": "5.0.0",
  "author": "Tim Carry (@pixelastic)",
  "description": "Async glob, read and write files",
  "keywords": [
    "shell",
    "filesystem",
    "copy",
    "move",
    "delete"
  ],
  "repository": "pixelastic/firost",
  "homepage": "https://projects.pixelastic.com/firost/",
  "type": "module",
  "sideEffects": false,
  "license": "MIT",
  "engines": {
    "node": ">=18.18.0"
  },
  "files": [
    "gitRoot/*.js",
    "newFile/index.js",
    "newFile/templates/*",
    "packageRoot/*.js",
    "watch/helpers/*.js",
    "watch/*.js",
    "*.js"
  ],
  "exports": {
    ".": "./main.js"
  },
  "main": "./main.js",
  "dependencies": {
    "@inquirer/prompts": "7.2.2",
    "callsites": "4.2.0",
    "chokidar": "4.0.3",
    "empathic": "1.0.0",
    "eventemitter2": "6.4.9",
    "execa": "5.1.1",
    "fs-extra": "11.2.0",
    "globby": "14.0.2",
    "golgoth": "3.0.0",
    "is-glob": "4.0.3",
    "json-stable-stringify": "1.2.1",
    "multimatch": "7.0.0",
    "nanoid": "5.0.9",
    "normalize-url": "8.0.1",
    "ora": "8.1.1",
    "rev-hash": "4.1.0",
    "std-mocks": "2.0.0",
    "strip-ansi": "7.1.0",
    "temp-dir": "3.0.0",
    "untildify": "5.0.0",
    "which": "5.0.0"
  },
  "devDependencies": {
    "get-port": "7.1.0"
  },
  "scripts": {
    "build": "ABERLAAS_CWD=$INIT_CWD yarn g:build",
    "build:prod": "ABERLAAS_CWD=$INIT_CWD yarn g:build:prod",
    "cms": "ABERLAAS_CWD=$INIT_CWD yarn g:cms",
    "serve": "ABERLAAS_CWD=$INIT_CWD yarn g:serve",
    "release": "ABERLAAS_CWD=$INIT_CWD yarn g:release",
    "test": "ABERLAAS_CWD=$INIT_CWD yarn g:test",
    "test:watch": "ABERLAAS_CWD=$INIT_CWD yarn g:test:watch",
    "compress": "ABERLAAS_CWD=$INIT_CWD yarn g:compress",
    "lint": "ABERLAAS_CWD=$INIT_CWD yarn g:lint",
    "lint:fix": "ABERLAAS_CWD=$INIT_CWD yarn g:lint:fix"
  }
}
