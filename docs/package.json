{
  "name": "firost-docs",
  "private": true,
  "type": "commonjs",
  "description": "Documentation website of firost",
  "dependencies": {
    "norska": "3.0.0",
    "norska-theme-docs": "5.0.3"
  },
  "scripts": {
    "build": "cd ../ && ./scripts/build",
    "build:prod": "cd ../ && ./scripts/build-prod",
    "cms": "cd ../ && ./scripts/cms",
    "compress": "cd ../ && ./scripts/compress",
    "lint": "cd ../ && ./scripts/lint",
    "lint:fix": "cd ../ && ./scripts/lint-fix",
    "release": "cd ../lib && yarn run release",
    "serve": "cd ../ && ./scripts/serve",
    "test": "cd ../lib && yarn run test",
    "test:watch": "cd ../lib && yarn run test:watch"
  }
}
